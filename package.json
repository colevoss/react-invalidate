{
  "name": "react-invalidate",
  "version": "1.1.1",
  "description": "Validation library for react based forms",
  "main": "./lib/index.js",
  "module": "./src/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "build:es": "BABEL_ENV=es babel src -d es",
    "build:watch": "npm run build -- --watch",
    "lint": "./node_modules/.bin/eslint src test",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"{src,test}/**/*.js\"",
    "flow": "./node_modules/.bin/flow",
    "test": "jest",
    "clean": "rimraf lib coverage",
    "prepublish": "npm run clean && npm run build",
    "test:cov": "npm run test -- --coverage && codecov",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [
    "react",
    "validator",
    "validation",
    "validate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/colevoss/react-invalidate.git"
  },
  "files": [
    "src",
    "lib"
  ],
  "author": "Cole Voss <voss.cole@gmail.com> (https://github.com/colevoss)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "codecov": "^1.0.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.1",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-react": "^6.10.0",
    "flow-bin": "^0.41.0",
    "jest": "^19.0.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.6.1"
  },
  "jest": {
    "coverageDirectory": "./coverage"
  },
  "dependencies": {
    "eslint-plugin-prettier": "^2.0.1",
    "prettier": "^0.22.0"
  }
}
