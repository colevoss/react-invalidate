{
  "presets": [
    "react",
    "es2015",
    "es2016"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "syntax-async-functions",
    ["transform-regenerator", {
      "asyncGenerators": false,
      "generators": true,
      "async": true
    }],
    "transform-runtime"
  ]
}
